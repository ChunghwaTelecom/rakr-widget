{
  "name": "rakr-widget",
  "version": "0.1.0",
  "description": "Widget enabling Rakr integration in any site.",
  "main": "dist/rakr.js",
  "typings": "dist/rakr.d.ts",
  "scripts": {
    "postinstall": "cd src && typings install",
    "prebuild": "rimraf dist",
    "build": "webpack --config webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "dev": "webpack-dev-server --content-base dist/",
    "clean": "rimraf node_modules && rimraf src/typings && rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChunghwaTelecom/rakr-widget.git"
  },
  "keywords": [
    "rakr",
    "widget",
    "client",
    "integration"
  ],
  "author": "Chunghwa Telecom Co., Ltd.",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/ChunghwaTelecom/rakr-widget/issues"
  },
  "homepage": "https://github.com/ChunghwaTelecom/rakr-widget#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "awesome-typescript-loader": "^0.17.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.3",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "core-js": "^2.4.0",
    "html2canvas": "^0.5.0-beta4",
    "mousetrap": "^1.5.3"
  }
}
